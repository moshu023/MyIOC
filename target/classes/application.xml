<?xml version="1.0" encoding="UTF-8"?>
<beans>
    <!-- xml 中，我们往 ioc 容器中放入了两个 bean，分别是 userService 和 userDao, userService 依赖 userDao-->
    <bean id="userService" class="com.hit.business.impl.UserServiceImpl">
        <property name="userDao" ref="userDao"/>
    </bean>
    <bean id="userDao" class="com.hit.business.impl.UserDaoImpl"/>
</beans>
